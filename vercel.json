{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "build": "npm run build"
    }
  ],
  "routes": [
    {
      "src": "/api/route",
      "dest": "/api/route.js"
    },
    {
      "src": "/queue/enqueue",
      "dest": "/queue/enqueue.js"
    },
    {
      "src": "/queue/process",
      "dest": "/queue/process.js"
    }
  ]
}
